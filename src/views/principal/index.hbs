	<!-- INICIO DE MODAL Elevador Infrarojo -->
   <div class="modal fade ModalMostarElevadorInfrarojo" tabindex="-1"  role="dialog" aria-labelledby="myExtraLargeModalLabel" aria-hidden="true">
      <div class="modal-dialog modal-xl" role="document" >      
        <div class="modal-content" >
          <div class="modal-header"> 
            <h1>Elevador Infrarrojo</h1>
            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
              <span aria-hidden="true">&times;</span>
            </button>
            </div>        
            <div class="modal-body" style="border:1px solid; height:400px; width:auto; overflow-y:scroll; overflow-x:hidden;" >
              <div style="border:1px solid; overflow-x:scroll; overflow-y:hidden;">        
                <table  class="table table-bordered" >
                  <thead class="thead-dark " >
                    <tr>
                      <th >ID</th>
                      <th >Apartado</th>
                      <th >Sensor</th>
                      <th >fecha</th>
                    </tr>
                  </thead>
                  <tbody>
                  {{#each Infrarojo}}
                    <tr>
                       <td>{{id}}</td>
                      <td>{{dataa.Apartado}}</td>
                      <td>{{dataa.Sensor}}</td>
                      <td>{{fecha}}</td>
                    </tr>
                        {{else}}
                  <h1>La tabra Elevador Infrarojo  esta basia</h1>
                  {{/each}}    
                  </tbody>
                </table>
              </div>
            </div>
          <div class="modal-footer">
            <button type="button" class="btn btn-primary" data-dismiss="modal">Cerrar</button>
          </div>
        </div>
      </div>
    </div>
	<!-- FIN DE MODAL Elevador Infrarojo -->
